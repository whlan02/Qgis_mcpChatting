{
  "mcpServers": {
      "filesystem": {
          "command": "npx",
          "args": [
              "-y",
              "@modelcontextprotocol/server-filesystem",
              "/ABSOLUTE/PATH/TO/YOUR/WORKING/DIRECTORY" 
          ]
      },
      "qgis": {
          "command": "uv",
          "args": [
              "--directory",
              "/ABSOLUTE/PATH/TO/PARENT/REPO/FOLDER/qgis_mcp",
              "run",
              "qgis_mcp_server.py"
          ]
      }
  }
}